<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update"
        android:id="@+id/buttonUpdate"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="17dp"/>

    <TextView
        android:text="Name"
        android:layout_height="wrap_content"
        android:id="@+id/PlaceLabel"
        android:layout_width="wrap_content"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <EditText
        android:inputType="textPersonName"
        android:text="Store Valby"
        android:ems="10"
        android:id="@+id/placeText"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        android:textSize="18sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="2dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/PlaceLabel"/>

    <EditText
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/snippetText"
        android:layout_width="325dp"
        android:text="Denmark"
        app:layout_constraintTop_toBottomOf="@+id/placeText"
        app:layout_constraintLeft_toLeftOf="@+id/placeText"
        app:layout_constraintStart_toStartOf="@+id/placeText"
        />

    <TextView
        android:text="Snippet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        app:layout_constraintBaseline_toBaselineOf="@+id/snippetText"
        app:layout_constraintRight_toLeftOf="@+id/snippetText"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="6dp"/>


    <Button
        android:text="Delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonDelete"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="17dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <Button
        android:text="Cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/buttonCancel"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="17dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:text="VOR coordinates"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:textSize="18sp"
        android:textColor="?android:attr/editTextColor"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORname1"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:width="100dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORname2"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/VORname1"
        android:width="100dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORname3"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/VORname2"
        android:width="100dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORrad1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/VORname1"
        android:width="90dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORdist1"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/VORrad1"
        android:width="110dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORrad2"
        android:width="90dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/VORname2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/VORrad1"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORdist2"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/VORdist1"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/VORrad2"
        android:width="110dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORrad3"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/VORname3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/VORrad2"
        android:width="90dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/VORdist3"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/VORrad3"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/VORdist2"
        android:width="110dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="Next WP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewNextWP"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/snippetText"
        android:textSize="18sp"
        android:textColor="?android:attr/editTextColor"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:text="Distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewNextWP"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:width="100dp"/>

    <TextView
        android:text="Heading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:width="100dp"/>

    <TextView
        android:text="nm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvDistance"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textViewNextWP"
        android:layout_marginTop="8dp"
        android:width="100dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="Ëš"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvHeading"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/tvDistance"
        android:layout_marginTop="8dp"
        android:width="100dp"
        android:textAlignment="viewEnd"/>

    <TextView
        android:text="Location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:textSize="18sp"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/VORname3"/>

    <TextView
        android:text="Lattitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        android:layout_marginTop="8dp"
        android:width="80dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvLat"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="@+id/textView14"/>

    <TextView
        android:text="Longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/tvLat"
        app:layout_constraintRight_toLeftOf="@+id/tvLon"
        android:layout_marginEnd="8dp"
        android:width="80dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvLon"
        app:layout_constraintTop_toTopOf="@+id/textView16"
        app:layout_constraintRight_toRightOf="@+id/VORdist3"/>

    <TextView
        android:text="Declination"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        android:width="80dp"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvVar"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19"/>


</android.support.constraint.ConstraintLayout>
