<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <TextView
        android:text="Position Report"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView27"
        android:textSize="18sp"
        android:textColor="@color/cast_expanded_controller_background_color"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:text="KarlslundeslaglilleKirkebakkevej"
        android:layout_width="328dp"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionName"
        android:width="300dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView27"
        android:background="@color/cast_intro_overlay_button_background_color"/>

    <TextView
        android:text="\@"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPTLabel"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewPositionName"/>

    <TextView
        android:text="00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionTime"
        android:background="@color/cast_intro_overlay_button_background_color"
        app:layout_constraintTop_toTopOf="@+id/textViewPTLabel"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textViewPTLabel"/>

    <TextView
        android:text="9999"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionAlt"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/textViewPositionTime"
        android:background="@color/cast_intro_overlay_button_background_color"/>

    <TextView
        android:text="Expect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView34"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewPTLabel"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <TextView
        android:text="KarlslundeslaglilleKirke"
        android:layout_width="277dp"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionNextName"
        app:layout_constraintTop_toTopOf="@+id/textView34"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textView34"
        android:background="@color/cast_intro_overlay_button_background_color"/>

    <TextView
        android:text="Then"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionThenLabel"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textViewPositionNextTimeLabel"/>

    <TextView
        android:text="KarlslundeslaglilleKirkebakkevej3"
        android:layout_width="276dp"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionThenName"
        android:background="@color/cast_intro_overlay_button_background_color"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/textViewPositionThenLabel"/>

    <TextView
        android:text="feet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView39"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textViewPositionAlt"
        app:layout_constraintTop_toTopOf="@+id/textViewPositionAlt"/>

    <Button
        android:text="OK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnPositionOK"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="68dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="68dp"/>

    <TextView
        android:text="00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionNextTime"
        android:background="@color/cast_intro_overlay_button_background_color"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/textViewPositionNextTimeLabel"
        app:layout_constraintTop_toTopOf="@+id/textViewPositionNextTimeLabel"/>

    <TextView
        android:text="\@"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionNextTimeLabel"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView34"/>

    <TextView
        android:text="9999"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionNextAlt"
        android:background="@color/cast_intro_overlay_button_background_color"
        app:layout_constraintLeft_toLeftOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/textViewPositionNextTime"/>

    <TextView
        android:text="feet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textViewPositionNextAltLabel"
        app:layout_constraintLeft_toRightOf="@+id/textViewPositionNextAlt"
        app:layout_constraintTop_toTopOf="@+id/textViewPositionNextAlt"
        android:layout_marginStart="8dp"/>

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        app:layout_constraintGuide_begin="345dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="345dp"/>
</android.support.constraint.ConstraintLayout>